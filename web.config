<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.webServer>
		<httpErrors errorMode="Custom">
			<remove statusCode="500" subStatusCode="-1" />
			<remove statusCode="404" subStatusCode="-1" />
			<error statusCode="404" prefixLanguageFilePath="" path="/Index.html" responseMode="ExecuteURL" />
			<error statusCode="500" prefixLanguageFilePath="" path="/Index.html" responseMode="ExecuteURL" />
			<error statusCode="500" subStatusCode="100" path="/Index.html" responseMode="ExecuteURL" />
		</httpErrors>
	</system.webServer>
	<system.web>
        <customErrors defaultRedirect="/Index.html" mode="On">
            <error redirect="/Index.html" statusCode="404" />
        </customErrors>
    </system.web>
</configuration>
